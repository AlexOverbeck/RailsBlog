<div class="row">
    <div class="col s6 offset-s3">
        <h2>Sign Up for ShMedium</h2>
    </div>
</div>
<% if @user.errors.any?  %>
    <h4>The Following Errors prevented the article form being created</h4>
    <ul>
        <% @user.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
        <% end %>
    </ul>
<% end %>

<%= render 'form' %>
